{
  "name": "ludotheque-ui",
  "version": "0.0.8",
  "scripts": {
    "build": "tsc",
    "build:watch": "run-singleton \"tsc -w --incremental --preserveWatchOutput\"",
    "start": "concurrently --names tsc,dev-server \"npm run build:watch\" \"web-dev-server --config ./web-dev-server.config.mjs\"",
    "package": "rm -rf ludotheque-ui.zip && rm -rf dist && tsc && rollup --config rollup.config.js && cd ./dist && cp * ../../../../electron/web && bestzip ../ludotheque-ui.zip *"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "18.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@web/dev-server": "^0.1.21",
    "@web/rollup-plugin-html": "^1.9.1",
    "@web/rollup-plugin-import-meta-assets": "^1.0.7",
    "babel-plugin-template-html-minifier": "^4.1.0",
    "bestzip": "^2.2.0",
    "concurrently": "^5.3.0",
    "deepmerge": "^4.2.2",
    "exits": "^2.0.1",
    "path-exists-cli": "^2.0.0",
    "rollup": "^2.56.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^6.2.0",
    "run-singleton-cli": "^0.0.5",
    "tslib": "^2.3.1",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "@holochain-open-dev/cell-client": "^0.3.2",
    "@holochain/client": "^0.3.2",
    "@holochain-open-dev/context": "^0.0.3",
    "@where/elements": "file:../../lib",
    "lit": "^2.1.1"
  }
}
