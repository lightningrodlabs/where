  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "start": "npm run build && concurrently --kill-others \"npm:start-ui\" \"npm:start-holochain\"",
    "start-ui": "CONDUCTOR_URL=ws://localhost:8888 concurrently -k --names tsc,dev-server \"npm run build-watch\" \"web-dev-server --config demo/web-dev-server.config.mjs\"",
    "start-holochain": "RUST_LOG=warn hc s generate -r=8888 ../zome/workdir/happ/where.happ network -b https://bootstrap-staging.holo.host quic",
    "build": "rimraf dist && rollup -c rollup.config.js",
    "build-watch": "rollup -c rollup.config.js -w",
    "test": "tsc && web-test-runner --coverage --puppeteer",
    "test-debug": "tsc && DEBUG=true web-test-runner --coverage --puppeteer",
    "test-watch": "web-test-runner --watch --puppeteer",
    "e2e": "CONDUCTOR_URL=ws://localhost:8888 concurrently -k -s first \"npm:test\" \"npm:start-holochain\"",
    "analyze": "cem analyze --litelement --globs \"src/elements/**/*\"",
    "lint": "eslint --ext .ts,.html . --ignore-path .gitignore",
    "format": "eslint --ext .ts,.html . --fix --ignore-path .gitignore",
    "storybook": "concurrently -k --names tsc,storybook \"npm run build-watch\" \"start-storybook --node-resolve --watch --open\"",
    "build-storybook": "npm run analyze && build-storybook",
    "publish-storybook": "npm run lint && npm run analyze && npm run build && npm run build-storybook && gh-pages -d storybook-static",
    "publish-to-branch": "npm run lint && npm run build && gh-pages -d ./ -b ui-build"
  },
  "dependencies": {
    "@holochain-open-dev/cell-client": "git+https://github.com/holochain-open-dev/cell-client.git#build",
    "@holochain-open-dev/context": "git+https://github.com/holochain-open-dev/context.git#ui-build",
    "@holochain-open-dev/core-types": "0.1.1",
    "@holochain-open-dev/profiles": "git+https://github.com/holochain-open-dev/profiles.git#ui-build",
    "@holochain/conductor-api": "0.2.0",
    "@lit-labs/context": "git+https://github.com/holochain-open-dev/context.git#build",
    "@open-wc/scoped-elements": "^2.0.0-next.4",
    "lit": "^2.0.0-rc.2",
    "lit-svelte-stores": "0.0.7",
    "mobx": "^6.3.3",
    "scoped-material-components": "^0.1.6"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.3.11",
    "@open-wc/demoing-storybook": "^2.0.0",
    "@open-wc/eslint-config": "^2.0.0",
    "@open-wc/testing": "^2.0.0",
    "@open-wc/testing-karma": "^3.0.0",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/node": "13.11.1",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "@web/dev-server": "^0.1.7",
    "@web/dev-server-rollup": "^0.3.2",
    "@web/test-runner": "^0.12.15",
    "@web/test-runner-puppeteer": "^0.6.4",
    "buffer": "^5.6.0",
    "concurrently": "^5.1.0",
    "cqfill": "^0.6.0",
    "deepmerge": "^3.2.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "gh-pages": "^3.1.0",
    "husky": "^1.0.0",
    "lint-staged": "^10.0.0",
    "postcss": "^8.3.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.50.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-postcss-lit": "^1.1.0",
    "tslib": "^1.11.0",
    "typescript": "~3.9.0"
  },
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "customElements": "custom-elements.json"
}
