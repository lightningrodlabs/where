{
  "name": "ludotheque",
  "version": "0.1.0",
  "main": "./out-tsc/index.js",
  "module": "./out-tsc/index.js",
  "typings": "./out-tsc/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "build": "rm -rf dist && tsc && rollup -c",
    "build:watch": "tsc -w --incremental --preserveWatchOutput",
    "start:ui": "concurrently --names tsc,dev-server \"npm run build:watch\" \"web-dev-server --config ./web-dev-server.config.mjs\"",
    "zip": "rm -rf ludotheque-web-ui.zip &&  cd dist && bestzip ../ludotheque-web-ui.zip *"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "18.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@web/dev-server": "^0.1.21",
    "@web/rollup-plugin-html": "^1.9.1",
    "@web/rollup-plugin-import-meta-assets": "^1.0.7",
    "babel-plugin-template-html-minifier": "^4.1.0",
    "bestzip": "^2.2.0",
    "concurrently": "^5.3.0",
    "deepmerge": "^4.2.2",
    "exits": "^2.0.1",
    "path-exists-cli": "^2.0.0",
    "rollup": "^2.56.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^6.2.0",
    "run-singleton-cli": "^0.0.5",
    "tslib": "^2.3.1",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "@ddd-qc/dna-client": "0.8.10",
    "@holochain/client": "^0.9.2",
    "@holochain-open-dev/utils": "0.4.2",
    "@holochain-open-dev/core-types": "^0.5.0",
    "@open-wc/scoped-elements": "^2.0.1",
    "@where/elements": "file:../webcomponents",
    "lit": "^2.1.1",
    "@lit-labs/context": "^0.1.2",
    "@lit/localize": "^0.11.2",
    "@scoped-elements/material-web": "^0.0.19"
  }
}
