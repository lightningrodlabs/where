{
  "name": "@where/app",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "build": "rm -rf dist && tsc && rollup -c && cd dist && cp -r * ../../electron/web",
    "build:watch": "tsc -w --incremental --preserveWatchOutput",
    "start:ui-old": "concurrently --names tsc,dev-server \"npm run build:watch\" \"web-dev-server --config ./web-dev-server.config.mjs\"",
    "start:ui": "web-dev-server --config ./web-dev-server.config.mjs",
    "localize:extract": "lit-localize extract",
    "localize:build": "lit-localize build",
    "zip": "rm -rf where-web-ui.zip && cd dist && bestzip ../where-web-ui.zip *",
    "package": "npm run zip && hc web-app pack ./webhapp.workdir"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "18.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@web/dev-server": "^0.1.21",
    "@web/rollup-plugin-html": "^1.9.1",
    "@web/rollup-plugin-import-meta-assets": "^1.0.7",
    "babel-plugin-template-html-minifier": "^4.1.0",
    "bestzip": "^2.2.0",
    "concurrently": "^5.3.0",
    "deepmerge": "^4.2.2",
    "exits": "^2.0.1",
    "path-exists-cli": "^2.0.0",
    "rollup": "^2.56.2",
    "run-singleton-cli": "^0.0.5",
    "tslib": "^2.4.0",
    "typescript": "^4.9.0"
  },
  "dependencies": {
    "@holochain/client": "0.11.15",
    "@ddd-qc/cell-proxy": "0.12.1",
    "@ddd-qc/lit-happ": "0.12.1",
    "@holochain-open-dev/core-types": "0.6.3",
    "@where/elements": "file:../webcomponents",
    "@open-wc/scoped-elements": "^2.1.1",
    "@scoped-elements/shoelace": "0.0.8",
    "@scoped-elements/material-web": "^0.0.19",
    "@lit-labs/context": "^0.1.2",
    "@lit/localize": "^0.11.2",
    "lit": "^2.3.1"
  }
}
